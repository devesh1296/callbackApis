{
  "workflowApiVersion": "1.1",
  "metaData": {
    "icon": "images/activity_icon.png",
    "category": "custom"
  },
  "type": "REST",
  "key": "REST-1",
  "lang": {
    "en-US": {
      "name": "Inbox And P13N Activity",
      "description": "An example REST activity using workflow API v1.1 format."
    }
  },
  "arguments": {
    "execute": {
      "inArguments": [
        {
          "emailAddress": "{{Contact.Attribute.P13_test_D.emailAddress}}",
          "FirstName": "{{Contact.Attribute.P13_test_D.FirstName}}",
          "LastName": "{{Contact.Attribute.P13_test_D.LastName}}",
          "PhoneNumber": "{{Contact.Attribute.P13_test_D.PhoneNumber}}",
          "UID": "{{Contact.Attribute.P13_test_D.UID}}",
          "UID_1": "{{Contact.Attribute.P13_test_D_1.UID}}",
          "CartId": "{{Contact.Attribute.P13_test_D_1.CartId}}",
          "ProductId": "{{Contact.Attribute.P13_test_D_1.ProductId}}"
        }
      ],
      "outArguments": [
        {
          "foundSignupDate": "default"
        }
      ],
      "url": "https://callbackapis.onrender.com/api/execute",
      "timeout": 20000
    }
  },
  "configurationArguments": {
    "save": {
      "url": "https://callbackapis.onrender.com/api/save"
    },
    "publish": {
      "url": "https://callbackapis.onrender.com/api/publish"
    }
  },
  "wizardSteps": [
    { "label": "Step 1", "key": "step1" },
    { "label": "Step 2", "key": "step2" },
    { "label": "Step 3", "key": "step3" },
    { "label": "Step 4", "key": "step4", "active": false }
  ],
  "userInterfaces": {
    "configModal": {
      "height": 200,
      "width": 300,
      "fullscreen": true
    }
  },
  "schema": {
    "arguments": {
      "execute": {
        "inArguments": [
          {
            "PhoneNumber": {
              "dataType": "Text",
              "isNullable": false,
              "direction": "in",
              "access": "visible"
            }
          },
          {
            "emailAddress": {
              "dataType": "Email",
              "isNullable": false,
              "direction": "in",
              "access": "visible"
            }
          },
          {
            "FirstName": {
              "dataType": "Text",
              "isNullable": false,
              "direction": "in",
              "access": "visible"
            }
          },
          {
            "LastName": {
              "dataType": "Text",
              "isNullable": false,
              "direction": "in",
              "access": "visible"
            }
          },
          {
            "UID": {
              "dataType": "Text",
              "isNullable": true,
              "direction": "in",
              "access": "visible"
            }
          },
          {
            "UID_1": {
              "dataType": "Text",
              "isNullable": false,
              "direction": "in",
              "access": "visible"
            }
          },
          {
            "CartId": {
              "dataType": "Text",
              "isNullable": true,
              "direction": "in",
              "access": "visible"
            }
          },
          {
            "ProductId": {
              "dataType": "Text",
              "isNullable": true,
              "direction": "in",
              "access": "visible"
            }
          }
        ],
        "outArguments": [
          {
            "foundSignupDate": {
              "dataType": "Text",
              "direction": "out",
              "access": "visible"
            }
          }
        ]
      }
    }
  }
}